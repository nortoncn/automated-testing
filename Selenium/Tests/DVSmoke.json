{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "verifyTextPresent",
      "text": "Search"
    },
    {
      "type": "verifyTextPresent",
      "text": "Download"
    },
    {
      "type": "verifyTextPresent",
      "text": "Upload"
    },
    {
      "type": "verifyTextPresent",
      "text": "Admin"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Search"
      }
    },
    {
      "type": "storeText",
      "step_name": "7.",
      "locator": {
        "type": "xpath",
        "value": "//div[1]/ui-view/ui-view/ul/li[1]/a/span[2]"
      },
      "variable": "NumberOfSamples"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[1]/ui-view/ui-view/ul/li[1]/a/span[2]"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Download"
      }
    },
    {
      "type": "print",
      "text": "${NumberOfSamples} Samples found by search"
    },
    {
      "type": "verifyTextPresent",
      "text": "There are no results to download. You must first search for the data of interest."
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Upload"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Other Upload"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "Tables"
    },
    {
      "type": "verifyTextPresent",
      "text": "Source"
    },
    {
      "type": "verifyTextPresent",
      "text": "Target"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Admin"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Jobs"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "Jobs"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='col-md-4']//button[.=' All']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='col-md-4']//button[.=' Failed/Active']"
      }
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}