{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "http://dv.pelabs.net:8503/"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "email"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "email"
      },
      "text": "egg@test.com"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='panel-body']//button[.='Login']"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "egg@test.com"
    },
    {
      "type": "storeText",
      "locator": {
        "type": "link text",
        "value": "egg@test.com"
      },
      "variable": "emailaddress"
    },
    {
      "type": "print",
      "text": "${emailaddress} logged in successfully"
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}