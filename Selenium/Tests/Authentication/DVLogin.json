{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "http://dv.pelabs.net:8502/#/securitymanager/signin"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "email"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "email"
      },
      "text": "egg@test.com"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "password"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "password"
      },
      "text": "test"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "ui-view.ng-scope > div.container.ng-scope"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='panel-body']//button[.='Login']"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "egg@test.com"
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}